# Copy this file to `keystore.properties` and populate it with the upload keystore credentials.
# `storeFile` may be absolute or relative to the project root. Do NOT commit the populated file.

# Default credentials (used when a flavor-specific block is not provided)
storeFile=
storePassword=
keyAlias=
keyPassword=

# Free flavor (com.free.pulselink) overrides
free.storeFile=Free-Certs/free-upload.jks
free.storePassword=
free.keyAlias=
free.keyPassword=

# Pro flavor (com.pulselink.pro) overrides
pro.storeFile=PRO-CERTS/pro-upload.jks
pro.storePassword=
pro.keyAlias=
pro.keyPassword=

# CI environments can alternatively provide values via UPLOAD_KEYSTORE_PATH(_FREE|_PRO),
# UPLOAD_KEYSTORE_PASSWORD(_FREE|_PRO), UPLOAD_KEY_ALIAS(_FREE|_PRO), and UPLOAD_KEY_PASSWORD(_FREE|_PRO).
