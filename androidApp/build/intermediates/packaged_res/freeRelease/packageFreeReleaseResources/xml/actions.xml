<?xml version="1.0" encoding="utf-8"?>
<actions>
    <action intentName="actions.intent.OPEN_APP_FEATURE">
        <fulfillment urlTemplate="pulselink://assistant/{feature}">
            <parameter-mapping
                intentParameter="feature"
                urlParameter="feature" />
        </fulfillment>

        <parameter name="feature">
            <entity-set-reference entitySetId="featureParam" />
        </parameter>
    </action>

    <entity-set entitySetId="featureParam">
        <entity
            identifier="emergency"
            name="Emergency"
            alternateName="@array/emergencySynonyms" />
        <entity
            identifier="cancel"
            name="Cancel"
            alternateName="@array/cancelSynonyms" />
    </entity-set>
</actions>
